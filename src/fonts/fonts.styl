@font-face
  font-family "Source Sans Pro"
  font-weight normal
  font-style normal
  src url("./SourceSansPro.eot")
  src local("☺"),
      url("./SourceSansPro.woff") format("woff"),
      url("./SourceSansPro.ttf") format("truetype"),
      url("./SourceSansPro.svg") format("svg")

@font-face
  font-family "Source Sans Pro"
  font-weight bold
  font-style normal
  src url("./SourceSansPro-Bold.eot")
  src local("☺"),
      url("./SourceSansPro-Bold.woff") format("woff"),
      url("./SourceSansPro-Bold.ttf") format("truetype"),
      url("./SourceSansPro-Bold.svg") format("svg")
